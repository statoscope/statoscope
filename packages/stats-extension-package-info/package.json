  ],
  "license": "MIT",
  "homepage": "https://github.com/statoscope/statoscope/",
  "dependencies": {
    "@statoscope/helpers": "^5.6.0"
  },
  "devDependencies": {
    "@statoscope/extensions": "^5.5.1",
    "@statoscope/stats": "^5.3.0"
  }
