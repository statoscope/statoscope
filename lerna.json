{
  "$schema": "node_modules/lerna/schemas/lerna-schema.json",
  "command": {
    "version": {
      "conventionalCommits": true,
      "message": "chore(release): %s",
      "push": false,
      "exact": true,
      "changelog": false
    }
  },
  "ignoreChanges": [
    "packages/*/README.md",
    "packages/*/package-lock.json",
    "packages/*/dist",
    "packages/*/**/*.spec.ts",
    "packages/*/**/*.snap"
  ],
  "version": "5.29.0"
}
